{
  "name": "@dx3/api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "db:migrate": "node --no-warnings -r dotenv/config -r @swc-node/register ./libs/pg/migrations/migrate.cli.ts up",
    "db:migrate:create": "node --no-warnings -r dotenv/config -r @swc-node/register ./libs/pg/migrations/migrate.cli.ts create",
    "db:migrate:down": "node --no-warnings -r dotenv/config -r @swc-node/register ./libs/pg/migrations/migrate.cli.ts down",
    "db:migrate:redo": "node --no-warnings -r dotenv/config -r @swc-node/register ./libs/pg/migrations/migrate.cli.ts redo",
    "db:migrate:status": "node --no-warnings -r dotenv/config -r @swc-node/register ./libs/pg/migrations/migrate.cli.ts status",
    "db:seed": "node --no-warnings -r dotenv/config -r @swc-node/register ./libs/pg/seed/seeder.ts",
    "db:seed:local": "POSTGRES_URI=postgres://pguser:password@localhost:5433/dx3 node --no-warnings -r dotenv/config -r @swc-node/register ./libs/pg/seed/seeder.ts",
    "lint": "biome check .",
    "test": "jest --detectOpenHandles",
    "test:libs": "NODE_OPTIONS='--no-deprecation' jest --testPathPattern=\"libs\" --detectOpenHandles",
    "test:libs:coverage": "NODE_OPTIONS='--no-deprecation' jest --testPathPattern=\"libs\" --coverage --detectOpenHandles",
    "test:libs:debug": "jest --testPathPattern=\"libs\" --debug",
    "test:libs:debug:coverage": "jest --testPathPattern=\"libs\" --debug --coverage",
    "test:libs:watch": "jest --testPathPattern=\"libs\" --watch",
    "test:libs:watch:coverage": "jest --testPathPattern=\"libs\" --watch --coverage"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.758.0",
    "@aws-sdk/lib-storage": "^3.758.0",
    "@aws-sdk/s3-request-presigner": "^3.948.0",
    "@dx3/encryption": "workspace:*",
    "@dx3/models-shared": "workspace:*",
    "@dx3/utils-shared": "workspace:*",
    "@sendgrid/client": "^8.1.3",
    "@sendgrid/mail": "^8.1.3",
    "@socket.io/redis-adapter": "^8.3.0",
    "axios": "^1.6.0",
    "connect-redis": "^8.0.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dayjs": "^1.11.13",
    "dotenv": "^17.2.3",
    "exponential-backoff": "^3.1.3",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "express-session": "^1.18.1",
    "express-winston": "^4.2.0",
    "formidable": "^3.5.2",
    "google-libphonenumber": "^3.2.43",
    "helmet": "^8.1.0",
    "http-status-codes": "^2.3.0",
    "ioredis": "^5.5.0",
    "joi": "^18.0.2",
    "jsonwebtoken": "^9.0.2",
    "libphonenumber-js": "^1.12.4",
    "lodash": "^4.17.21",
    "obscenity": "^0.4.5",
    "pg": "^8.13.3",
    "rate-limit-redis": "^4.2.0",
    "reflect-metadata": "^0.2.2",
    "sequelize": "^6.37.5",
    "sequelize-typescript": "^2.1.6",
    "sharp": "^0.33.5",
    "socket.io": "^4.8.1",
    "socket.io-adapter": "^2.5.5",
    "uuid": "^11.1.0",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "zxcvbn-typescript": "^5.0.1"
  },
  "devDependencies": {
    "@dx3/test-data": "workspace:*",
    "@types/connect-redis": "^0.0.23",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.18.0",
    "@types/express-winston": "^4.0.0",
    "@types/formidable": "^3.4.5",
    "@types/google-libphonenumber": "^7.4.30",
    "@types/ioredis": "^5.0.0",
    "@types/ioredis-mock": "^8.2.5",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/lodash": "^4.17.5",
    "@types/node": "^22.10.2",
    "aws-sdk-client-mock": "^4.1.0",
    "esbuild": "^0.19.2",
    "ioredis-mock": "^8.13.1",
    "jest-express": "^1.12.0",
    "nodemon": "^3.1.10",
    "socket.io-client": "^4.8.1"
  }
}
