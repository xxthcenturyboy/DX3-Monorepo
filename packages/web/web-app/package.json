{
  "name": "@dx3/web-app",
  "version": "0.0.0",
  "private": true,
  "main": "../web-app-dist/main.js",
  "types": "../web-app-dist/main.d.ts",
  "scripts": {
    "build": "rspack build --mode production",
    "build:analyze": "ANALYZE=true rspack build --mode production",
    "build:ssr": "rspack build --config rspack.config.server.js --mode production && rspack build --config rspack.config.client.js --mode production",
    "build:ssr:client": "rspack build --config rspack.config.client.js --mode development",
    "build:ssr:server": "rspack build --config rspack.config.server.js --mode development",
    "dev": "rspack serve --mode development",
    "dev:ssr": "npm run build:ssr:server && npm run build:ssr:client && npm run start:ssr",
    "dev:ssr:watch": "nodemon --watch src --ext ts,tsx,js,jsx --exec 'npm run build:ssr:server && npm run start:ssr'",
    "lint": "biome check .",
    "start:ssr": "node ../web-app-dist/ssr/server.js",
    "test": "NODE_OPTIONS='--no-deprecation' jest --detectOpenHandles --testPathIgnorePatterns='/libs/'",
    "test:coverage": "NODE_OPTIONS='--no-deprecation' jest --coverage --detectOpenHandles--testPathIgnorePatterns='/libs/'",
    "test:watch": "NODE_OPTIONS='--no-deprecation' jest --watch --testPathIgnorePatterns='/libs/'",
    "test:watch:coverage": "NODE_OPTIONS='--no-deprecation' jest --watch --coverage --testPathIgnorePatterns='/libs/'",
    "test:debug": "NODE_OPTIONS='--no-deprecation' jest --debug --testPathIgnorePatterns='/libs/'",
    "test:debug:coverage": "NODE_OPTIONS='--no-deprecation' jest --debug --coverage --testPathIgnorePatterns='/libs/'"
  },
  "dependencies": {
    "@dx3/models-shared": "workspace:*",
    "@dx3/utils-shared": "workspace:*",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@fingerprintjs/fingerprintjs": "^5.0.1",
    "@fontsource/roboto": "^5.2.0",
    "@mdxeditor/editor": "^3.25.0",
    "@mui/icons-material": "^7.3.6",
    "@mui/material": "^7.3.6",
    "@popperjs/core": "^2.11.8",
    "@reduxjs/toolkit": "^2.6.0",
    "axios": "^1.6.0",
    "compression": "^1.8.1",
    "dayjs": "^1.11.13",
    "jwt-decode": "^4.0.0",
    "libphonenumber-js": "^1.12.4",
    "lottie-react": "^2.4.1",
    "mui-one-time-password-input": "^5.0.0",
    "react": "^19.1.4",
    "react-avatar-editor": "^13.0.2",
    "react-dom": "^19.1.4",
    "react-is": "^19.1.4",
    "react-markdown": "^10.1.0",
    "react-phone-input-2": "^2.15.1",
    "react-redux": "^9.2.0",
    "react-router": "^7.2.0",
    "react-spinners": "^0.15.0",
    "react-toastify": "^11.0.5",
    "react-transition-group": "^4.4.5",
    "redux": "^5.0.1",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^3.1.0",
    "reduxjs-toolkit-persist": "^7.2.1",
    "rehype-sanitize": "^6.0.0",
    "reselect": "^5.1.1",
    "socket.io-client": "^4.8.1",
    "uuid": "^11.1.0",
    "zxcvbn-typescript": "^5.0.1"
  },
  "devDependencies": {
    "@dx3/test-data": "workspace:*",
    "@emotion/cache": "^11.14.0",
    "@emotion/server": "^11.11.0",
    "@rspack/cli": "^1.1.5",
    "@rspack/core": "^1.1.5",
    "@rspack/dev-server": "1.0.9",
    "@rspack/plugin-react-refresh": "^1.0.0",
    "@testing-library/dom": "10.4.0",
    "@testing-library/react": "^16.2.0",
    "@types/compression": "^1.8.1",
    "@types/cookie-parser": "^1.4.10",
    "@types/express": "^4.17.25",
    "@types/node": "^25.0.9",
    "@types/react": "^19.1.4",
    "@types/react-avatar-editor": "^13.0.3",
    "@types/react-dom": "^19.1.4",
    "@types/react-is": "^19.0.0",
    "@types/react-transition-group": "^4.4.10",
    "cookie-parser": "^1.4.7",
    "css-loader": "^7.1.2",
    "dotenv": "^17.2.3",
    "dotenv-webpack": "^8.1.1",
    "express": "^4.21.2",
    "nodemon": "^3.1.11",
    "postcss": "^8.4.40",
    "postcss-loader": "^8.1.1",
    "react-refresh": "~0.14.0",
    "style-loader": "^4.0.0",
    "stylus": "^0.63.0",
    "webpack-bundle-analyzer": "^4.10.2"
  }
}
